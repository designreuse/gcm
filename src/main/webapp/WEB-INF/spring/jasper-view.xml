<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

    <bean id="pais_report"
          class="org.springframework.web.servlet.view.jasperreports.JasperReportsMultiFormatView"
          p:url="/WEB-INF/reports/pais.jasper"
          p:reportDataKey="datasource"/>

    <bean id="movimentofinanceiro_programacao_report"
          class="org.springframework.web.servlet.view.jasperreports.JasperReportsMultiFormatView"
          p:url="/WEB-INF/reports/MovimentoFinanceiro.jasper"
          p:reportDataKey="datasource"/>

    <bean id="movimentofinanceiro_programacao_vencimento_report"
          class="org.springframework.web.servlet.view.jasperreports.JasperReportsMultiFormatView"
          p:url="/WEB-INF/reports/movimentofinanceiro_programacao_vencimento.jasper"
          p:reportDataKey="datasource"/>

    <bean id="movimentofinanceiro_programacao_pessoa_report"
          class="org.springframework.web.servlet.view.jasperreports.JasperReportsMultiFormatView"
          p:url="/WEB-INF/reports/movimentofinanceiro_programacao_pessoa.jasper"
          p:reportDataKey="datasource"/>

    <bean id="movimentofinanceiro_programacao_conta_report"
          class="org.springframework.web.servlet.view.jasperreports.JasperReportsMultiFormatView"
          p:url="/WEB-INF/reports/MovimentoFinanceiro_programacao_conta.jasper"
          p:reportDataKey="datasource"/>
</beans>